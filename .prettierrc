{
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "bracketSameLine": false,
  "singleQuote": true,
  "printWidth": 120,
  "semi": false,
  "plugins": [
    "prettier-plugin-organize-imports"
  ]
}