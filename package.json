{
  "name": "reactjs-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://telegram-mini-apps.github.io/reactjs-template",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "worker:dev": "wrangler dev src/worker/index.ts",
    "worker:build": "esbuild src/worker/index.ts --bundle --outfile=dist/worker.js --format=esm",
    "deploy": "npm run build && npm run worker:build && wrangler publish"

  },
  "dependencies": {
    "@nanostores/i18n": "^0.12.2",
