    "@telegram-apps/sdk-react": "^1.0.0",
    "@telegram-apps/telegram-ui": "^2.1.5",
    "@tonconnect/ui-react": "^2.0.5",
    "axios": "^1.7.5",
    "crypto": "^1.0.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "eruda": "^3.0.1",
    "gsap": "^3.12.5",
    "nanoevents": "^9.0.0",
    "nanostores": "^0.11.2",
    "posthog-js": "^1.157.2",
    "prop-types": "^15.8.1",
    "proton-engine": "^6.0.3",
    "raf-manager": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.1",
    "react-query-kit": "^3.3.0",
    "react-router-dom": "^6.26.1",
    "react-toastify": "^10.0.5",
    "react-tooltip": "^5.28.0",
